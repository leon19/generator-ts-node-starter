{
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Current TS File",
        "type": "node",
        "request": "launch",
        "args": ["${relativeFile}"],
        "runtimeArgs": ["-r", "ts-node/register"],
        "sourceMaps": true,
        "cwd": "${workspaceRoot}"
      }
    ]
  },
  "prettier.enable": true,
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}
